<?xml version="1.0" ?>

<linkage_config>

	<DBMS>PostgreSQL</DBMS>

	<DatabaseSourceA>
		<database>rosita</database>
		<table>source1a_encrypted</table>
		<schema>tz</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSourceA>

	<DatabaseSourceB>
		<database>rosita</database>
		<table>source1b_encrypted</table>
		<schema>tz</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSourceB>

	<DatabaseSaver>
		<database>rosita</database>
		<schema>tz</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSaver>

	<LinkageConfig>
		<acceptancelevel>95</acceptancelevel>
		<weightidenfifier>EM</weightidenfifier>
		<joinmethod>NESTED_LOOP_JOIN</joinmethod>
		<missingdatavalue>DISTANCE_IMPUTATION</missingdatavalue>
	</LinkageConfig>

	
	<DatabaseMap>

	<MappedPair attr="DOB" algorithm="PPRL" weight="0.2">
			<read_col>s_dob_hash</read_col>
			<write_col>s_dob_hash</write_col>
		</MappedPair>
	<MappedPair attr="MOB" algorithm="PPRL" weight="0.2">
			<read_col>s_mob_hash</read_col>
			<write_col>s_mob_hash</write_col>
		</MappedPair>
	<MappedPair attr="YOB" algorithm="PPRL" weight="0.2">
			<read_col>s_yob_hash</read_col>
			<write_col>s_yob_hash</write_col>
		</MappedPair>
	<MappedPair attr="sex" algorithm="PPRL" weight="0.2">
			<read_col>sex_hash</read_col>
			<write_col>sex_hash</write_col>
		</MappedPair>
	<MappedPair attr="LastName" algorithm="PPRL" weight="0.2">
			<read_col>last_hash</read_col>
			<write_col>last_hash</write_col>
		</MappedPair>
	<MappedPair attr="FirstName" algorithm="PPRL" weight="0.2">
			<read_col>first_hash</read_col>
			<write_col>first_hash</write_col>
		</MappedPair>
	<MappedPair attr="City" algorithm="PPRL" weight="0.2">
			<read_col>city_hash</read_col>
			<write_col>city_hash</write_col>
		</MappedPair>
		
	</DatabaseMap>
	<Linking>
		<left_pk>id</left_pk>
		<right_pk>id</right_pk>
	</Linking>
	
</linkage_config>
