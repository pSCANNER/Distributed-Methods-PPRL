<?xml version="1.0" ?>

<linkage_config>

	<DBMS>PostgreSQL</DBMS>

	<DatabaseSourceA>
		<database>rosita</database>
		<table>chcb_clinical_encrypted</table>
		<schema>tz2</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSourceA>

	<DatabaseSourceB>
		<database>rosita</database>
		<table>chcb_claims_encrypted</table>
		<schema>tz2</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSourceB>

	<DatabaseSaver>
		<database>rosita</database>
		<schema>tz2</schema>
		<url>jdbc:postgresql://localhost:5432/</url>
		<user>rosita</user>
		<password>rosita@2012</password>
	</DatabaseSaver>

	<LinkageConfig>
		<acceptancelevel>85</acceptancelevel>
		<weightidenfifier>EM</weightidenfifier>
		<joinmethod>BLOCKING_SEARCH</joinmethod>
		<missingdatavalue>0</missingdatavalue>
	</LinkageConfig>

	
	<DatabaseMap>

	<MappedPair attr="DOB" algorithm="PPRL" weight="0.2">
			<read_col>s_dob_hash</read_col>
			<write_col>s_dob_hash</write_col>
		</MappedPair>
	<MappedPair attr="MOB" algorithm="PPRL" weight="0.2">
			<read_col>s_mob_hash</read_col>
			<write_col>s_mob_hash</write_col>
		</MappedPair>
	<MappedPair attr="Blocking" algorithm="PPRL" weight="0.2">
			<read_col>s_yob_hash</read_col>
			<write_col>s_yob_hash</write_col>
		</MappedPair>
	<MappedPair attr="sex" algorithm="PPRL" weight="0.2">
			<read_col>gender_hash</read_col>
			<write_col>gender_hash</write_col>
		</MappedPair>
	<MappedPair attr="LastName" algorithm="PPRL" weight="0.2">
			<read_col>last_hash</read_col>
			<write_col>last_hash</write_col>
		</MappedPair>
	<MappedPair attr="FirstName" algorithm="PPRL" weight="0.2">
			<read_col>first_hash</read_col>
			<write_col>first_hash</write_col>
		</MappedPair>
	<MappedPair attr="Zip" algorithm="EDIT_DISTANCE" weight="0.2">
			<read_col>zip_hash</read_col>
			<write_col>zip_hash</write_col>
		</MappedPair>
		
	</DatabaseMap>

	<EMBlocking>
		<read_col>s_yob_hash</read_col>
		<write_col>s_yob_hash</write_col>
	</EMBlocking>

	<Blocking>
		<read_col>s_yob_hash</read_col>
		<write_col>s_yob_hash</write_col>
	</Blocking>

	
	<Linking>
		<left_pk>id</left_pk>
		<right_pk>id</right_pk>
	</Linking>
	
</linkage_config>

